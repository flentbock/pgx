{
    "name": "pgx",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "s": "ng serve",
        "start": "ng serve -o",
        "build": "ng build",
        "test": "ng test --code-coverage",
        "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
        "coveralls": "eal ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "pretty-quick": "pretty-quick",
        "pre-commit2": "pretty-quick --staged",
        "prepushx": "npm test",
        "precommit": "npm run commitlint && npm run lint",
        "precommit2": "npm run pretty-quick --staged && npm run commitlint && npm run lint && npm run pretty-quick --staged",
        "commitlint": "commitlint -e",
        "cc": "npm run prettier && npm run lint",
        "prettier": "prettier --write src/**/*{ts,css,md}"


    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^7.1.3",
