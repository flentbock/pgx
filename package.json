        "@commitlint/config-conventional": "^7.1.2",
        "@types/jasmine": "~3.3.1",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~10.12.12",
        "angular-ide": "^0.9.52",
        "codelyzer": "~4.5.0",
        "coveralls": "^3.0.2",
        "husky": "^1.2.0",
        "jasmine": "^3.3.1",
        "jasmine-core": "~3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.3",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
